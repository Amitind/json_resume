{{#settings}}
	{{#glyphicons}}
		<link rel="stylesheet" type="text/css" href="public/css/bootstrap.min.css" />
		<script>document.getElementById('github-png').innerHTML = '<img src="public/images/github.png" style="width:22px"></img> ';
		</script>
	{{/glyphicons}}
{{/settings}}
<link rel="stylesheet" type="text/css" href="public/css/screen.css" />
<div id="page">
{{#bio_data}}
	<div class="contact-details">
		{{#email}}
		<span class="glyphicon glyphicon-envelope"></span> {{email}}<br/>
		{{/email}}
		{{#phone}}
		<span class="glyphicon glyphicon-earphone"></span> {{phone}}<br/>
		{{/phone}}
		{{#website}}
		<span class="glyphicon glyphicon-globe"></span>
		<a href="{{website}}"> {{website}}</a>
		{{/website}}
	</div>
	
	<h1 class="emphnext">{{firstname}} {{familyname}}</h1><br/>
	{{#stars}}
	<span class="stars">
		 {{#items}}
		 {{name}} {{^last}}<span class="glyphicon glyphicon-star-empty"></span>{{/last}}
		 {{/items}}
	</span>
	{{/stars}}
	<br/>

	{{#summary}}
	<h2><span class="glyphicon glyphicon-compressed"></span> ##summary##</h2>
	<ul>
		{{#points}}
		<li>{{{.}}}</li>
		{{/points}}
	</ul>
	{{/summary}}

	{{#github_projects}}
	<h2><span id="github-png"></span>##github_projects##</h2>
	<ul>
	{{#details}}
		<li><a href="https://github.com/{{project_name}}">{{project_name}}</a>
		{{#tagline}} &raquo; {{{tagline}}}{{/tagline}}
			<ul>
				{{#description}}
				<li>{{{.}}}</li>
				{{/description}}
			</ul>
		</li>
	{{/details}}
	</ul>
	{{/github_projects}}

	{{#skills}}
	<h2><span class="glyphicon glyphicon-th-list"></span> ##skills##</h2>
	<ul class="languages">
		{{#details}}
		<li>{{type}}: 
			{{#items}}
			<span class="label label-default">{{name}}</span>
			{{/items}}
		</li>
		{{/details}}
	</ul>
	{{/skills}}

	{{#education}}
	<h2><span class="glyphicon glyphicon-book"></span> ##education##</h2>
	<table class="table education">
		<tr>
			<th>##degree##</th><th>##major##</th><th>##institution##</th><th nowrap>##graduation_year##</th>{{#show_gpa}}<th>##gpa##</th>{{/show_gpa}}
		</tr>
		{{#schools}}
		<tr>
			<td>{{degree}}</td><td>{{major}}</td><td>{{institution}}</td><td align="center">{{graduation_year}}</td>{{#show_gpa}}<td>{{gpa}}</td>{{/show_gpa}}
		</tr>
		{{/schools}}
	</table>
	{{/education}}

	{{#acad_achievements}}
	<h2><span class="glyphicon glyphicon-tower"></span> ##acad_achievements##</h2>
	<ul>
		{{#items}}
		<li>{{{.}}}</li>
		{{/items}}
	</ul>
	{{/acad_achievements}}

	{{#experience}}
	<h2><span class="glyphicon glyphicon-briefcase"></span> ##experience##</h2>
	<ul>
		{{#items}}
		<li>{{title}}, {{{organisation}}}{{#location}}, {{{location}}}{{/location}}
			<span class="right">({{from}} – {{to}})</span>
			<ul>
				{{#details}}
				<li>{{{.}}}</li>
				{{/details}}
				{{#technology_used}}
				<li>##technology_used##: 
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}
			</ul>
		</li>
		{{/items}}
	</ul>
	{{/experience}}

	{{#other_projects}}
	<h2><span class="glyphicon glyphicon-cog"></span> ##other_projects##</h2>
	<ul>
		{{#items}}
		<li>
			{{{headline}}}
			<ul>
				{{#points}}
				<li>{{{.}}}</li>
				{{/points}}
				{{#technology_used}}
				<li>##technology_used##: 
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}			
			</ul>
		</li>
		{{/items}}
	</ul>
	{{/other_projects}}

	{{#grad_courses}}
	<h2><span class="glyphicon glyphicon-bookmark"></span> ##grad_courses##</h2>
	<table class="table education">
		{{#rows}}
		<tr>
			{{#columns}}
			<td><a href="{{url}}">{{name}}</a></td>
			{{/columns}}
		</tr>
		{{/rows}}
	</table>
	{{/grad_courses}}

	{{#undergrad_courses}}
	<h2><span class="glyphicon glyphicon-bookmark"></span> ##undergrad_courses##</h2>
	<table class="table education">
		{{#rows}}
		<tr>
			{{#columns}}
			<td><a href="{{url}}">{{name}}</a></td>
			{{/columns}}
		</tr>
		{{/rows}}
	</table>
	{{/undergrad_courses}}

	{{#research_experience}}
	<h2><span class="glyphicon glyphicon-pencil"></span> ##research_experience##</h2>
	<ul>
		{{#items}}
		<li>
			{{title}}, {{{organisation}}}
			<span class="right">({{from}} – {{to}})</span>
			<ul>
				{{#points}}
				<li>{{{.}}}</li>
				{{/points}}
			</ul>
		</li>
		{{/items}}
	</ul>
	{{/research_experience}}

	{{#patents}}
	<h2><span class="glyphicon glyphicon-file"></span> ##patents##</h2>
	<ul>
		{{#items}}
		<li>{{patent_id}} 
			<a href="{{url}}">{{title}}</a> 
			{{#issuance_date}}, ##issuance## {{issuance_date}}{{/issuance_date}}.
		</li>
		{{/items}}
	</ul>
	{{/patents}}

	{{#papers}}
	<h2><span class="glyphicon glyphicon-edit"></span> ##papers##</h2>
	<ul>
		{{#items}}
		<li>{{authors}} 
			<a href="{{url}}">{{title}},</a> 
			 {{{misc}}}.
		</li>
		{{/items}}
	</ul>
	{{/papers}}

	{{#memberships}}
	<h2><span class="glyphicon glyphicon-globe"></span> ##memberships##</h2>
	<ul>
		{{#committees}}
		<li>{{{.}}}</li>
		{{/committees}}
	</ul>	
	{{/memberships}}
	
	{{#extra_curricular}}
	<h2><span class="glyphicon glyphicon-music"></span> ##extra_curricular##</h2>
	<ul>
		{{#items}}
		<li>{{{.}}}</li>
		{{/items}}
	</ul>
	{{/extra_curricular}}

{{/bio_data}}
</div><!-- End Page -->
